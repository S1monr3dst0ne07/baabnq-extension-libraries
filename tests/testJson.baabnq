    put __ <- _pNode + _Json::Node::String; jump fail ~ __ != 0;
    put __ <- _pNode + _Json::Node::Number; jump fail ~ __ != 0;
    put __ <- _pNode + _Json::Node::Name;   jump fail ~ __ != 0;

    jump succ;


