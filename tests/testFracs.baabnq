lab TestFracs::Neg;
    sub Local::TestFracs::Init;

    push 0;
    push 0; 
    push 0; 
    sub Fracs::CreateByComponents;
    sub Fracs::Neg;
    pull _pFrac;

    put __ <- _pFrac + _Fracs::Frac::SIGN;
    jump fail ~ __ != 1;
    
    push _pFrac;
    sub Fracs::Neg;
    sub Stack::Drop;

    put __ <- _pFrac + _Fracs::Frac::SIGN;
    jump fail ~ __ != 0;

    jump succ;

