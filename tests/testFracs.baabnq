    put __ <- _pFrac + _Fracs::Frac::SIGN;  jump fail ~ __ != 1;
    put __ <- _pFrac + _Fracs::Frac::NUMER; jump fail ~ __ != 2;
    put __ <- _pFrac + _Fracs::Frac::DENER; jump fail ~ __ != 3;

    jump succ;

